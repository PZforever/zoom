page{
  height: 94%;
  background-color: #f7f6f6;
}
.nav{
  width: 94%;
  padding: 16rpx 3% 20rpx 3%;
  background: #fff;
  position: relative;
  border-top: 20rpx solid #f7f6f6;
}
.nav>view{
  display: inline-block;
}
.nav .line_img{
   width: 20rpx;
   height: 22rpx;
   position: relative;
   top: 2rpx;
   margin-right: 20rpx; 
}
.nav .choose{
  width: 188rpx; 
  height:40rpx;
  line-height:40rpx;
  font-size: 30rpx;
  text-align: center; 
  cursor: pointer;
}
.phone_one{
  display: flex; 
  position: relative;
  margin:0 auto;
}
.phone_personal{
  color:#999;
  background: #fff;
}
.select_one{
  text-align: center;
  background:#fff; 
  width:190rpx;
  border-radius: 3px;
  font-size: 30rpx;
}
.personal_image{
  z-index: 1;
  position: absolute;
  right:6rpx;
  top: 16rpx;
  width: 16rpx;
  height: 12rpx; 
  transition: All 0.4s ease;
}
.outRotate{
  transform: rotate(180deg);
}
.nav .edit{
   display: inline-block;
   float: right;
   margin-top: 6rpx;
   color: #fa822b;
   font-size: 31rpx;
   font-weight: 700;
   margin-right: 20rpx;
}
.nav .edit image{
   width: 30rpx;
   height: 30rpx;
   margin-right: 5rpx;
}

/* 内容 */
.body{
  width: 100%;
  border-top: 10rpx solid #f7f6f6;
  background: #FFF;
}
.body>view{
  width: 100%;
  /* line-height: 85rpx; */
  background: #fff;
  position: relative;
}
.head{
  line-height: 85rpx;
  border-bottom: 1px solid #EEE;
}
.head_left{
  overflow: hidden;
  margin-left: 31rpx;
  width: 50%;
}
.head checkbox{
  zoom: 70%;
}
.head checkbox,.head_title,.head_img{
  float: left
}
.head_img{
  margin-left: 15rpx;
}
.head .head_title{
  display: block;
  font-size: 30rpx;
  max-width: 60%;
}
.head .head_title text{
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow:ellipsis;
}
.head_left image{
  width: 26rpx;
  height: 30rpx;
}
.rightFath{
  position: absolute;
  top: 0;
  right: 44rpx;
  font-size: 31rpx;
}
.head_right{
  color: #fa822b;
  display:inline;
}
.head_right image{
  width: 26rpx;
  height: 26rpx;
  margin-right: 15rpx;
}
.cutOut{
  display: inline-block;
}
.cutOut>image{
  width: 45rpx;
  height: 25rpx;
}
.cutOut>view{
  display: inline-block;
  width: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
  text-align: center;
  color: #ffF;
  background: #ff9a00;
  border-radius: 50%;
  font-weight: 600;
  margin:0 20rpx 0 45rpx;
  font-size: 30rpx;
}
.content{
  line-height: 100rpx;
  background: #fff;
  padding-left: 15rpx;
  border-bottom: 1px solid #eee;
  position: relative
}
.content_box{
  display: inline-block;
  margin-left:40rpx;
  margin-right:10rpx;
  width: 50rpx;
}
.cent_nav{
  display: inline-block;
  width: 80%
}
.page_check{
  zoom: 70%;
  float: left;
  margin-left:40rpx;
  margin-right:10rpx;
} 
.cont_text{
  font-size: 30rpx;
  color: #666;
  height: 96rpx;
}

.cont_submit{
  width: 115rpx;
  height: 30rpx;
  margin-left: 20rpx;
}
.contHover{
  display: none
}
 .check_nav{
  position: relative;
 
}
.cont_time{
  margin-left:0rpx;
  padding:0px;
  margin-top:-60rpx;
   font-size: 26rpx;
  color: #999; 
}
.pic{
   position: absolute;
   right: 44rpx;
   top: 53rpx;
}
/* 底部删除 */
.page__bd .out{
  height: 85rpx;
  color: #999;
  font-size: 24rpx;
  position: relative;
  border-bottom: 1px solid #d7d7d7;
  overflow: hidden
}
.out>view{
  display: inline-block;
}
.outBtn{
  float: right; 
  margin-top: 16rpx; 
  margin-right: 44rpx;  
  height: 56rpx;
}
.outBtn>view{
  display: inline-block;
}
.outBtn>.promptBox{
  position: relative;
  top: -10rpx;
  font-size:26rpx;
  font-weight:700;
  color:red;
  margin-left:10rpx;
}
.outBtn>button{
  display: inline-block;
}
.out_left{
  margin-left: 31rpx;
  line-height:85rpx;
}
.out_left text{
  color: red;
}
.outSub{
  /* display: none; */
  width: 90rpx;
  height: 48rpx;
  color: #fff;
  background: #ffb800;
  font-size: 24rpx;
  border-radius: 6rpx;
  padding: 0;
  font-weight: 700;
  line-height: 50rpx;
  margin-right: 25rpx;
}
.out_right{
  width: 140rpx;
  height: 50rpx;
  font-size: 24rpx;
  color: #999;
  border-radius: 6rpx;
  padding: 0;
  font-weight: 700;
  line-height: 50rpx;
}
.right_change{
  background: #fa822b;
  color: #fff;
  border: 1px solid #fa822b;
}
.sunNone{
  display:block
}
.hidden{
  display: none;
}

.isMore{
  font-size: 38rpx;
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  color: #666;
  text-align: center;
}
.markUrl{
  width:20rpx;
  height:31rpx;
  margin-left:10rpx;
  position:relative;
  top:5rpx;
}

