/* 蒙层 */
.modalDialog2 {
  width: 650rpx;
  /* position: fixed; */
  top: 400rpx;  
  z-index: 9999;
  background: #fff;
  border-radius: 5rpx;
}
 .modalContent{
  padding: 20rpx;
  font-size: 35rpx;
  color: #666;
  position: relative
} 
.triangle{
  position: absolute;
  left: 45%;
  height: 0px;
  width: 0px;
  border-top: 20rpx solid #fff;
  border-right: 20rpx solid transparent;
  border-left: 20rpx solid transparent;
}

/* 门窗蒙层 */
.modal-mask {
width: 100%; 
height: 100%;
position: fixed;
top: 0;
left: 0;
background:rgba(0, 0, 0, .6);
overflow: hidden;
z-index: 9000;
display:flex;
display:-webkit-flex;
justify-content:center;
-webkit-justify-content:center;
align-items:center;
-webkit-align-items:center;
}
.modalDialog {
width: 640rpx;
height: 540rpx;
overflow: hidden;
/* position: fixed; */
z-index: 9999;
background: #fff;
border-radius: 10rpx;
}
