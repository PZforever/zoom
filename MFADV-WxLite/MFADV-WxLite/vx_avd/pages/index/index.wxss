.list{
  width: 100%;
  height: 40%;
  background: #fafafa;
  box-sizing: border-box;
  overflow-y: scroll;
  -webkit-overflow-scrolling : touch;
}

.mainScreen{
  height: 70%;
}

.list .item{
  width: 100%;
  background: #FFF;
  box-sizing: border-box;
  border-top: 1px solid #efefef;
  border-bottom: 1px solid #efefef;
  padding: 10rpx 30rpx;
  margin-bottom: 15rpx;
}

.list .item.activeItem{
  box-shadow: 0 0 10rpx 10rpx #fad9c0
}

.list .item:first-child{
  border-top: none;
}

.list .item:last-child{
  border-bottom: none;
  margin: 0;
}

.list .item .item-name{
  font-size: 32rpx;
  font-weight: bold;
  line-height:64rpx;
  position: relative;
}

.list .item .item-name .check{
  position: absolute;
  top: 0;
  right: 0;
  height: 64rpx;
  line-height: 64rpx;
}

.list .item .item-name .check text{
  display: block;
  width: 40rpx;
  height: 40rpx;
  margin-top: 12rpx;
  border: 1px solid #aaa;
}

.list .item .item-name .check .ischecked{
  background: #fa8327;
  border: none;
  line-height: 40rpx;
  text-align: center;
  color: #FFF;
}

.list .item .item-name .check .ischecked image{
  display: block;
  width: 34rpx;
  height: 34rpx; 
}

.list .item .item-info{
  color: #777;
  overflow: hidden;
}

.list .item .item-info text:first-child{
  float: left;
}

.list .item .item-info text:last-child{
  float: right;
}

.list .item .item-edit{
  margin: 20rpx 0;
  font-size: 28rpx;
  color: #999;
}

.list .item .item-edit .audio{
  width: 100%;
  box-sizing: border-box;
  display: flex;
  display:-webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  align-items: center;
  justify-content: flex-start;
}

.list .item .item-edit .audio .handle{
  width: 10%;
  /* float: left; */
  /* display: table; */
}

.list .item .item-edit .audio .handle text{
  /* display: table-cell; */
}

.list .item .item-edit .audio .handle .play{
  display: block;
  width: 0; 
  height: 0; 
  border-top: 20rpx solid transparent; 
  border-bottom: 20rpx solid transparent; 
  border-left: 40rpx solid #fa8327; 
}

.list .item .item-edit .audio .handle .stop{
  display: block;
  width: 12rpx; 
  height: 40rpx;  
  border-left: 10rpx solid #fa8327;
  border-right: 10rpx solid #fa8327; 
}

.list .item .item-edit .audio .process{
  width: 90%;
  /* float: left; */
  /* margin-left: 15rpx; */
  display: flex;
  display:-webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  align-items: center;
  justify-content: space-between;
}

.list .item .item-edit .audio .process text{
  display: inline-block;
  width:19%;
}

.list .item .item-edit .audio .process progress{
  width: 60%;
}

.list .item .item-edit .audio .process slider{
  width: 60%;
}

.list .item .item-edit .audio .process .length{
  width: 85%;
  height: 12rpx;
  background: #DDD;
  margin: 0 25rpx;
  position: relative;
}

.list .item .item-edit .audio .process .length .bar{
  position: absolute;
  width: 0;
  height: 24rpx;
  top: -6rpx;
  background: #ec6100;
  left: 0;
}

.list .item .item-edit .upload{
  margin-top: 20rpx;
  border-top: 1px solid #EEE;
  overflow: hidden;
}

.list .item .item-edit .upload text{
  display: block;
  color: #f98227;
  margin-top: 15rpx;
}

.list .item .item-edit .upload text:first-child{
  float: left;
}

.list .item .item-edit .upload text:last-child{
  float: right;
}

