page{
  background-color: #F7F6F6;
}
.options{
  width: 700rpx;
  margin: 25rpx;
  height: 120rpx;
  background: white;
}
.checked{
  height: 120rpx;
  color: #F9812B;
  border-radius: 6rpx;
  border: 2rpx solid;
  border-color: #FCEFE6;
}
.unchecked{
  height: 120rpx;
}
.circle{
  display: inline-block;
  margin-top: 35rpx;
  margin-left: 120rpx; 
  width: 100rpx;
  height: 120rpx;
}
.circle image{
  width: 35rpx;
  height: 35rpx;
}
.checked_name{
  display: inline-block;
  font-size: 40rpx;
  height: 120rpx;
}
.unchecked_name{
  display: inline-block;
  font-size: 40rpx;
  height: 120rpx;
}
button{
  margin: 60rpx;
  color: white;
  background: #F9812B;
}