/**app.wxss**/
@import 'style/weui.wxss';
page{
  background-color: #F7F6F6;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}  
.clearfix:after {
    content: ".";
    display: block;
    height: 0;
    clear: both;
    visibility: hidden;
}

.fl{
    float: left;
}

.fr{
    float: right;
}
/*确定按钮  */
.subBtn{
  width: 600rpx;
  height: 80rpx;
  margin: 60rpx auto;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  font-size: 30rpx;
  background-color: #fa822b;
}
/*确定按钮 end  */

